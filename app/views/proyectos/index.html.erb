<h1>Listing proyectos</h1>

<table class="table table-bordered">
  <tr>
    <th>Nombre</th>
    <th>Cliente</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @proyectos.each do |proyecto| %>
  <tr>
    <td><%= proyecto.nombre %></td>
    <td><%= proyecto.cliente %></td>
    <td><%= link_to 'Show', proyecto, class: "btn btn-primary btn-large"  %></td>
    <td><%= link_to 'Edit', edit_proyecto_path(proyecto), class: "btn btn-info btn-large" %></td>
    <td><%= link_to 'Destroy', proyecto, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger btn-large" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Proyecto', new_proyecto_path, class: "btn btn-inverse btn-large" %>
